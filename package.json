{
  "name": "org-index-graph",
  "description": "Org index graph",
  "repository": "github:ben-walker/org-index-graph",
  "license": "BSD-3-Clause",
  "engines": {
    "node": "16"
  },
  "scripts": {
    "build": "tsc --project ./tsconfig.build.json",
    "dev": "./scripts/dev.sh",
    "down": "./scripts/down.sh",
    "generate:schema": "ts-node ./scripts/generate-schema",
    "lint": "eslint . --ext .ts,.js",
    "prebuild": "prisma generate",
    "start": "node ./dist/app",
    "start:dev": "ts-node-dev --exit-child --respawn ./src/app",
    "typecheck": "tsc --noEmit"
  },
  "prisma": {
    "seed": "ts-node ./prisma/seed"
  },
  "dependencies": {
    "@prisma/client": "~3.12.0",
    "apollo-server-express": "^3.6.5",
    "argon2": "^0.28.5",
    "class-validator": "^0.13.2",
    "express": "^4.17.2",
    "graphql": "^15.3.0",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.14.1",
    "jsonwebtoken": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "tslib": "^2.3.1",
    "type-graphql": "^1.1.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.1.2",
    "@types/graphql-fields": "^1.3.4",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^18.6.5",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-type-graphql": "^1.0.0",
    "prettier": "^2.5.1",
    "prisma": "~3.12.0",
    "ts-node": "^10.5.0",
    "ts-node-dev": "^1.1.8",
    "typegraphql-prisma": "^0.20.0",
    "typescript": "^4.5.5"
  }
}
