datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

generator typegraphql {
  provider = "typegraphql-prisma"
}

model User {
  createdAt DateTime @default(now())
  email     String   @unique
  id        String   @id @default(cuid())
}
