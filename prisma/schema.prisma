datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

generator typegraphql {
  provider = "typegraphql-prisma"
}

model Organization {
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  name      String
}

model User {
  createdAt       DateTime  @default(now())
  email           String    @unique
  emailVerifiedAt DateTime?
  id              String    @id @default(cuid())
  name            String
}
